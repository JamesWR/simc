warrior="Warrior_Protection_T17N"
level=100
race=night_elf
role=tank
position=front
talents=http://us.battle.net/wow/en/tool/talent-calculator#Zb!0110211
glyphs=unending_rage/death_from_above/bull_rush/cleave
spec=protection

# This default action priority list is automatically created based on your character.
# It is a attempt to provide you with a action list that is both simple and practicable,
# while resulting in a meaningful and good simulation. It may not result in the absolutely highest possible dps.
# Feel free to edit, adapt and improve it to your own needs.
# SimulationCraft is always looking for updates and improvements to the default action lists.

# Executed before combat begins. Accepts non-harmful actions only.

actions.precombat=flask,type=greater_draenor_critical_strike_flask
actions.precombat+=/food,type=blackrock_barbecue
# IF YOU WISH TO SIMULATE YOUR CHARACTER AS GLADIATOR, PLEASE CHANGE ROLE TO DPS IN GLOBAL OPTIONS, AND THEN RE-IMPORT FROM BATTLE.NET
# AFTER IMPORTING, MAKE SURE YOUR LEVEL IS SET TO 100 AND GLADIATORS RESOLVE IS TALENTED. 
# EXAMPLE TALENT LINE, THE VERY LAST DIGIT (2) IS GLADIATORS RESOLVE: talents=http://us.battle.net/wow/en/tool/talent-calculator#Zb!0102212
actions.precombat+=/stance,choose=defensive
# Snapshot raid buffed stats before combat begins and pre-potting is done.
actions.precombat+=/snapshot_stats
actions.precombat+=/potion,name=draenor_armor

# Executed every time the actor is available.

actions=charge
actions+=/auto_attack
actions+=/use_item,name=primal_gladiators_badge_of_victory
actions+=/potion,name=draenor_armor,if=(target.health.pct<20&buff.recklessness.up)|target.time_to_die<=25
actions+=/heroic_strike,if=buff.ultimatum.up
actions+=/shield_block
actions+=/shield_barrier,if=incoming_damage_1500ms>health.max*0.3|rage>rage.max-20
actions+=/shield_wall,if=incoming_damage_2500ms>health.max*0.6
actions+=/last_stand,if=incoming_damage_2500ms>health.max*0.6&buff.shield_wall.down
actions+=/run_action_list,name=normal_rotation

actions.normal_rotation=shield_slam
actions.normal_rotation+=/revenge
actions.normal_rotation+=/enraged_regeneration,if=talent.enraged_regeneration.enabled
actions.normal_rotation+=/heroic_strike,if=buff.ultimatum.up
actions.normal_rotation+=/ravager,if=talent.ravager.enabled
actions.normal_rotation+=/thunder_clap
actions.normal_rotation+=/demoralizing_shout
actions.normal_rotation+=/impending_victory,if=talent.impending_victory.enabled
actions.normal_rotation+=/victory_rush,if=!talent.impending_victory.enabled
actions.normal_rotation+=/bloodbath,if=talent.bloodbath.enabled
actions.normal_rotation+=/bladestorm,if=talent.bladestorm.enabled
actions.normal_rotation+=/shockwave,if=talent.shockwave.enabled
actions.normal_rotation+=/storm_bolt,if=talent.storm_bolt.enabled
actions.normal_rotation+=/dragon_roar,if=talent.dragon_roar.enabled
actions.normal_rotation+=/devastate

